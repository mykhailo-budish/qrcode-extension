{
  "name": "QR Code Scanner",
  "action": {},
  "manifest_version": 3,
  "version": "0.2",
  "description": "Makes QR Codes accessible for navigation.",
  "permissions": [
    "activeTab",
    "clipboardWrite",
    "contextMenus",
    "scripting"
  ],
  "background": {
    "service_worker": "background.js"
  },
  "icons": {
    "16": "icons/qrcode-icon-16.png",
    "48": "icons/qrcode-icon-48.png",
    "128": "icons/qrcode-icon-128.png"
  }
}
